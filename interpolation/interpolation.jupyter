code 6
    def dumb_linear_interpolation(x, fraction):
        "This is a very dumb implementation!"
        x = np.asarray(x)
        assert x.ndim == 1, "x must be one-dimensional!"
        
        y_len = len(x) - 1
        y = np.empty(y_len)
        for i in range(y_len):
            y[i] = (1 - fraction) * x[i] + fraction * x[i + 1]
        return y
